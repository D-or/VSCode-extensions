{
	"name": "word-count",
    "displayName": "Word Count",
    "description": "Count the number of word for all file type",
    "version": "0.1.3",
    "publisher": "DoubleWoodH",
    "engines": {
        "vscode": "^1.32.0"
    },
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"*"
	],
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}

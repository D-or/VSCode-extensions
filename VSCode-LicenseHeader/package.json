{
    "name": "licenseheader",
    "displayName": "LicenseHeader",
    "description": "custom MIT license header",
    "version": "1.0",
    "publisher": "DoubleWoodH",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.licenseHeader"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "License header Configuration",
            "properties": {
                "licenseheader.LicenseName": {
                    "type": "string",
                    "default": "MIT",
                    "description": ""
                },
                "licenseheader.LicenseUrl": {
                    "type": "string",
                    "default": "https://raw.githubusercontent.com/facebook/react-native/master/LICENSE",
                    "description": ""
                },
                "licenseheader.Author": {
                    "type": "string",
                    "default": "DoubleWoodH",
                    "description": "By default, created by."
                },
                "licenseheader.CopiedBy": {
                    "type": "string",
                    "default": "Copyright (c) 2017 DoubleWoodH",
                    "description": "By default, copied by."
                }
            }
        },
        "keybindings": [
            {
                "command": "extension.licenseHeader",
                "key": "ctrl+shift+i",
                "mac": "cmd+shift+i",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}
